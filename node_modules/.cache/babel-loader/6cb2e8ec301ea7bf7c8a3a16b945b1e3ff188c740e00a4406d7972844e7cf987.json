{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"Header\",\n  created() {},\n  methods: {\n    show() {\n      this.dialogVisible = true;\n      this.myInformation = {};\n      request.get(\"/user/information\").then(res => {\n        this.myInformation = res.data;\n      });\n    }\n  },\n  data() {\n    return {\n      dialogVisible: false,\n      myInformation: {}\n    };\n  }\n};","map":{"version":3,"mappings":"AAwEA,OAAOA,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,OAAO,GAAG,CACV,CAAC;EACDC,OAAO,EAAE;IACPC,IAAI,GAAG;MACL,IAAI,CAACC,aAAY,GAAI,IAAG;MACxB,IAAI,CAACC,aAAY,GAAI,CAAC;MACtBN,OAAO,CAACO,GAAG,CAAC,mBAAmB,CAAC,CAACC,IAAI,CAACC,GAAG,IAAE;QACzC,IAAI,CAACH,aAAY,GAAIG,GAAG,CAACC,IAAG;MAC9B,CAAC;IACH;EACF,CAAC;EACDA,IAAI,GAAG;IACL,OAAO;MACLL,aAAa,EAAE,KAAK;MACpBC,aAAa,EAAE,CAAC;IAClB;EACF;AAEF","names":["request","name","created","methods","show","dialogVisible","myInformation","get","then","res","data"],"sourceRoot":"","sources":["E:\\move\\springboot-vue-demo\\vue\\src\\components\\Header.vue"],"sourcesContent":["<template>\r\n  <div style=\"height: 80px;line-height: 50px;border-bottom:1px solid #ccc;display: flex\">\r\n    <div style=\"width: 200px;padding-left: 30px;padding-top: 10px;\r\n    font-size:x-large;font-weight: bold;color: dodgerblue\">管理员\r\n    </div>\r\n    <div style=\"flex: 1\"></div>\r\n    <div style=\"width: 100px;padding-top: 25px\">\r\n      <el-dropdown>\r\n    <span class=\"el-dropdown-link\" style=\"font-size: x-large\">\r\n      张三\r\n      <el-icon class=\"el-icon--right\">\r\n        <arrow-down/>\r\n      </el-icon>\r\n    </span>\r\n        <template #dropdown>\r\n          <el-dropdown-menu>\r\n            <el-dropdown-item @click=\"show\">个人信息</el-dropdown-item>\r\n            <el-dropdown-item @click=\"$router.push('/login')\">退出系统</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </template>\r\n      </el-dropdown>\r\n      <el-dialog title=\"个人信息\" v-model=\"dialogVisible\" width=\"35%\">\r\n        <el-form :model=\"myInformation\" label-width=\"15%\">\r\n          <el-form-item required=\"required\" label=\"用户名\">\r\n            <el-input v-model=\"myInformation.username\" clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"密码\">\r\n            <el-input v-model=\"myInformation.password\" show-password clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"权限\">\r\n            <el-radio v-model=\"myInformation.status\" label=0>管理员</el-radio>\r\n            <el-radio v-model=\"myInformation.status\" label=1>普通用户</el-radio>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"昵称\">\r\n            <el-input v-model=\"myInformation.nickName\" clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"年龄\">\r\n            <el-input v-model=\"myInformation.age\" clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"性别\">\r\n            <el-radio v-model=\"myInformation.sex\" label=\"男\">男</el-radio>\r\n            <el-radio v-model=\"myInformation.sex\" label=\"女\">女</el-radio>\r\n            <el-radio v-model=\"myInformation.sex\" label=\"未知\">未知</el-radio>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"地址\">\r\n            <el-input type=\"textarea\" v-model=\"myInformation.address\" maxlength=\"30\" show-word-limit></el-input>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"邮箱\">\r\n            <el-input v-model=\"myInformation.email\" clearable>\r\n              <template #suffix>\r\n                <el-button v-model:disabled=\"myInformation\" text type=\"primary\" plain @click=\"send\">发送验证码</el-button>\r\n              </template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"验证码\">\r\n            <el-input v-model=\"myInformation.emailCode\" clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button style=\"margin-left: 20%; width: 20%; text-align: center; font-weight: bold\" type=\"success\"\r\n                       @click=\"register\">修改\r\n            </el-button>\r\n            <el-button style=\"width: 20%; text-align: center; font-weight: bold\" type=\"info\"\r\n                       @click=\"this.dialogVisible = false\">取消\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Header\",\r\n  created() {\r\n  },\r\n  methods: {\r\n    show() {\r\n      this.dialogVisible = true\r\n      this.myInformation = {}\r\n      request.get(\"/user/information\").then(res=>{\r\n        this.myInformation = res.data\r\n      })\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      myInformation: {}\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}