{"ast":null,"code":"// @ is an alias to /src\n\nimport request from \"@/utils/request\";\nexport default {\n  name: 'User',\n  components: {},\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          search: this.search,\n          pageSize: this.pageSize,\n          pageNum: this.pageNum\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    my() {\n      this.dialogVisible = true;\n      this.myData = [];\n      request.get(\"/orders\").then(res => {\n        this.myData = res.data;\n      });\n    },\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/user\", this.form).then(res => {\n          console.log(res);\n          if (res.code === 1) {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        //新增\n        request.post(\"/user\", this.form).then(res => {\n          console.log(res);\n          if (res.code === 1) {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n    },\n\n    handleSizeChange() {\n      //改变当前每页个数\n      this.load();\n    },\n    handleCurrentChange() {\n      //改变当前页码\n      this.load();\n    },\n    buy(id) {\n      request.get(\"/orders/\" + id).then(res => {\n        if (res.code === 1) {\n          this.$message({\n            type: \"success\",\n            message: \"购票成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.load(); //重新加载删除后的数据\n      });\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/user/\" + id).then(res => {\n        if (res.code === 1) {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.load(); //重新加载删除后的数据\n      });\n    }\n  },\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      search: '',\n      tableData: [],\n      myData: []\n    };\n  }\n};","map":{"version":3,"mappings":"AA4DA;;AAEA,OAAOA,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE,CAAC,CAAC;EACdC,OAAO,GAAG;IACR,IAAI,CAACC,IAAI,EAAC;EACZ,CAAC;EACDC,OAAO,EAAE;IACPD,IAAI,GAAG;MACLJ,OAAO,CAACM,GAAG,CAAC,OAAO,EAAE;QACnBC,MAAM,EAAE;UACNC,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,OAAO,EAAE,IAAI,CAACA;QAChB;MACF,CAAC,CAAC,CAACC,IAAI,CAACC,GAAE,IAAK;QACbC,OAAO,CAACC,GAAG,CAACF,GAAG;QACf,IAAI,CAACG,SAAQ,GAAIH,GAAG,CAACI,IAAI,CAACC,OAAM;QAChC,IAAI,CAACC,KAAI,GAAIN,GAAG,CAACI,IAAI,CAACE,KAAI;MAC5B,CAAC;IACH,CAAC;IACDC,EAAE,GAAG;MACH,IAAI,CAACC,aAAY,GAAI,IAAG;MACxB,IAAI,CAACC,MAAK,GAAI,EAAC;MACfrB,OAAO,CAACM,GAAG,CAAC,SAAS,CAAC,CAACK,IAAI,CAACC,GAAG,IAAE;QAC/B,IAAI,CAACS,MAAK,GAAIT,GAAG,CAACI,IAAG;MACvB,CAAC;IACH,CAAC;IACDM,IAAI,GAAG;MACL,IAAI,IAAI,CAACC,IAAI,CAACC,EAAE,EAAE;QAAC;QACjBxB,OAAO,CAACyB,GAAG,CAAC,OAAO,EAAE,IAAI,CAACF,IAAI,CAAC,CAACZ,IAAI,CAACC,GAAE,IAAK;UAC1CC,OAAO,CAACC,GAAG,CAACF,GAAG;UACf,IAAIA,GAAG,CAACc,IAAG,KAAM,CAAC,EAAE;YAClB,IAAI,CAACC,QAAQ,CAAC;cACZC,IAAI,EAAE,SAAS;cACfC,OAAO,EAAE;YACX,CAAC;UACH,OAAO;YACL,IAAI,C