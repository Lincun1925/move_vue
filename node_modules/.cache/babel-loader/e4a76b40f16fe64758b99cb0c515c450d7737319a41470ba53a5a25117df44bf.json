{"ast":null,"code":"// @ is an alias to /src\n\nimport request from \"@/utils/request\";\nexport default {\n  name: 'Home',\n  components: {},\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      console.log(this.search);\n      request.get(\"/car\", {\n        params: {\n          search: this.search,\n          pageSize: this.pageSize,\n          pageNum: this.pageNum\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/car\", this.form).then(res => {\n          console.log(res);\n          if (res.code === 1) {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        //新增\n        request.post(\"/car\", this.form).then(res => {\n          console.log(res);\n          if (res.code === 1) {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n          this.load(); //刷新表格数据\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n    },\n\n    handleSizeChange() {\n      //改变当前每页个数\n      this.load();\n    },\n    handleCurrentChange() {\n      //改变当前页码\n      this.load();\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/car/\" + id).then(res => {\n        if (res.code === 1) {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.load(); //重新加载删除后的数据\n      });\n    }\n  },\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      search: \"\",\n      tableData: []\n    };\n  }\n};","map":{"version":3,"mappings":"AA2FA;;AAEA,OAAOA,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE,CAAC,CAAC;EACdC,OAAO,GAAG;IACR,IAAI,CAACC,IAAI,EAAC;EACZ,CAAC;EACDC,OAAO,EAAE;IACPD,IAAI,GAAG;MACLE,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,MAAM;MACvBR,OAAO,CAACS,GAAG,CAAC,MAAM,EAAE;QAClBC,MAAM,EAAE;UACNF,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBG,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,OAAO,EAAE,IAAI,CAACA;QAChB;MACF,CAAC,CAAC,CAACC,IAAI,CAACC,GAAE,IAAK;QACbR,OAAO,CAACC,GAAG,CAACO,GAAG;QACf,IAAI,CAACC,SAAQ,GAAID,GAAG,CAACE,IAAI,CAACC,OAAM;QAChC,IAAI,CAACC,KAAI,GAAIJ,GAAG,CAACE,IAAI,CAACE,KAAI;MAC5B,CAAC;IACH,CAAC;IACDC,GAAG,GAAG;MACJ,IAAI,CAACC,aAAY,GAAI,IAAG;MACxB,IAAI,CAACC,IAAG,GAAI,CAAC;IACf,CAAC;IACDC,IAAI,GAAG;MACL,IAAI,IAAI,CAACD,IAAI,CAACE,EAAE,EAAE;QAAC;QACjBvB,OAAO,CAACwB,GAAG,CAAC,MAAM,EAAE,IAAI,CAACH,IAAI,CAAC,CAACR,IAAI,CAACC,GAAE,IAAK;UACzCR,OAAO,CAACC,GAAG,CAACO,GAAG;UACf,IAAIA,GAAG,CAACW,IAAG,KAAM,CAAC,EAAE;YAClB,IAAI,CAACC,QAAQ,CAAC;cACZC,IAAI,EAAE,SAAS;cACfC,OAAO,EAAE;YACX,CAAC;UACH,OAAO;YACL,IAAI,CAACF,QAAQ,CAAC;cACZC,IAAI,EAAE,OAAO;cACbC,OAAO,EAAEd,GAAG,CAACe;YACf,CAAC;UACH;UACA,IAAI,CAACzB,IAAI,EAAC,EAAE;UACZ,IAAI,CAACgB,aAAY,GAAI,KAAI,EAAE;QAC7B,CAAC;MACH,OAAO;QAAC;QACNpB,OAAO,CAAC8B,IAAI,CAAC,MAAM,EAAE,IAAI,CAACT,IAAI,CAAC,CAACR,IAAI,CAACC,GAAE,IAAK;UAC1CR,OAAO,CAACC,GAAG,CAACO,GAAG;UACf,IAAIA,GAAG,CAACW,IAAG,KAAM,CAAC,EAAE;YAClB,IAAI,CAACC,QAAQ,CAAC;cACZC,IAAI,EAAE,SAAS;cACfC,OAAO,EAAE;YACX,CAAC;UACH,OAAO;YACL,IA