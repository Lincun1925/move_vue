{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: \"Header\",\n  created() {},\n  methods: {\n    show() {\n      this.dialogVisible = true;\n      this.myInformation = {};\n      request.get(\"/user/information\").then(res => {\n        this.myInformation = res.data;\n      });\n    },\n    send() {\n      this.disabled = true;\n      request.get(\"/user/sendMsg/\" + this.formRegister.email).then(res => {\n        if (res.code === 1) {\n          this.$message({\n            type: \"success\",\n            message: \"验证码发送成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n    register() {\n      request.post(\"/user/register\", this.formRegister).then(res => {\n        if (res.code === 1) {\n          this.$message({\n            type: \"success\",\n            message: \"注册成功，欢迎\" + res.data.nickName\n          });\n          if (res.data.status === 0) {\n            //管理员登录\n            this.$router.push(\"/home\"); //登录成功进行页面跳转，进入主页\n          } else {\n            //普通用户\n            this.$router.push(\"/user\");\n          }\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    }\n  },\n  data() {\n    return {\n      dialogVisible: false,\n      myInformation: {},\n      disabled: false\n    };\n  }\n};","map":{"version":3,"mappings":";AAwEA,OAAOA,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,OAAO,GAAG,CACV,CAAC;EACDC,OAAO,EAAE;IACPC,IAAI,GAAG;MACL,IAAI,CAACC,aAAY,GAAI,IAAG;MACxB,IAAI,CAACC,aAAY,GAAI,CAAC;MACtBN,OAAO,CAACO,GAAG,CAAC,mBAAmB,CAAC,CAACC,IAAI,CAACC,GAAG,IAAE;QACzC,IAAI,CAACH,aAAY,GAAIG,GAAG,CAACC,IAAG;MAC9B,CAAC;IACH,CAAC;IACDC,IAAI,GAAG;MACL,IAAI,CAACC,QAAO,GAAI,IAAG;MACnBZ,OAAO,CAACO,GAAG,CAAC,gBAAe,GAAI,IAAI,CAACM,YAAY,CAACC,KAAK,CAAC,CAACN,IA