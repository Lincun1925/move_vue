tem @click=\"$router.push('/login')\">退出系统</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </template>\r\n      </el-dropdown>\r\n      <el-dialog title=\"个人信息\" v-model=\"dialogVisible\" width=\"35%\">\r\n        <el-form :model=\"myInformation\" label-width=\"15%\">\r\n          <el-form-item required=\"required\" label=\"用户名\">\r\n            <el-input v-model=\"myInformation.username\" clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"密码\">\r\n            <el-input v-model=\"myInformation.password\" show-password clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"权限\">\r\n            <el-radio v-model=\"myInformation.status\" label=\"0\">管理员</el-radio>\r\n            <el-radio v-model=\"myInformation.status\" label=\"1\">普通用户</el-radio>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"昵称\">\r\n            <el-input v-model=\"myInformation.nickName\" clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"年龄\">\r\n            <el-input v-model=\"myInformation.age\" clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"性别\">\r\n            <el-radio v-model=\"myInformation.sex\" label=\"男\">男</el-radio>\r\n            <el-radio v-model=\"myInformation.sex\" label=\"女\">女</el-radio>\r\n            <el-radio v-model=\"myInformation.sex\" label=\"未知\">未知</el-radio>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"地址\">\r\n            <el-input type=\"textarea\" v-model=\"myInformation.address\" maxlength=\"30\" show-word-limit></el-input>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"邮箱\">\r\n            <el-input v-model=\"myInformation.email\" clearable>\r\n              <template #suffix>\r\n                <el-button v-model:disabled=\"disabled\" text type=\"primary\" plain @click=\"send\">发送验证码</el-button>\r\n              </template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item required=\"required\" label=\"验证码\">\r\n            <el-input v-model=\"myInformation.emailCode\" clearable></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button style=\"margin-left: 20%; width: 20%; text-align: center; font-weight: bold\" type=\"success\"\r\n                       @click=\"register\">修改\r\n            </el-button>\r\n            <el-button style=\"width: 20%; text-align: center; font-weight: bold\" type=\"info\"\r\n                       @click=\"this.dialogVisible = false\">取消\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Header\",\r\n  created() {\r\n  },\r\n  methods: {\r\n    show() {\r\n      this.dialogVisible = true\r\n      this.myInformation = {}\r\n      request.get(\"/user/information\").then(res=>{\r\n        this.myInformation = res.data\r\n      })\r\n    },\r\n    send() {\r\n      this.disabled = true\r\n      request.get(\"/user/sendMsg/\" + this.formRegister.email).then(res => {\r\n        if (res.code === 1) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"验证码发送成功\"\r\n          })\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n    register() {\r\n      request.post(\"/user/register\", this.formRegister).then(res => {\r\n        if (res.code === 1) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"注册成功，欢迎\" + res.data.nickName\r\n          })\r\n          if (res.data.status === 0) {//管理员登录\r\n            this.$router.push(\"/home\")//登录成功进行页面跳转，进入主页\r\n          } else {//普通用户\r\n            this.$router.push(\"/user\")\r\n          }\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      myInformation: {},\r\n      disabled: false\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}