{"ast":null,"code":"// @ is an alias to /src\n\nimport request from \"@/utils/request\";\nexport default {\n  name: 'User',\n  components: {},\n  created() {\n    this.load();\n  },\n  methods: {\n    out() {},\n    load() {\n      request.get(\"/user\", {\n        params: {\n          search: this.search,\n          pageSize: this.pageSize,\n          pageNum: this.pageNum\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    my() {\n      this.dialogVisible = true;\n      this.myData = [];\n      request.get(\"/orders\").then(res => {\n        this.myData = res.data;\n        console.log(this.myData);\n      });\n    },\n    handleSizeChange() {\n      //改变当前每页个数\n      this.load();\n    },\n    handleCurrentChange() {\n      //改变当前页码\n      this.load();\n    },\n    buy(id) {\n      request.get(\"/orders/\" + id).then(res => {\n        if (res.code === 1) {\n          this.$message({\n            type: \"success\",\n            message: \"购票成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.load(); //重新加载删除后的数据\n      });\n    }\n  },\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      search: '',\n      tableData: [],\n      myData: []\n    };\n  }\n};","map":{"version":3,"mappings":"AA4DA;;AAEA,OAAOA,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE,CAAC,CAAC;EACdC,OAAO,GAAG;IACR,IAAI,CAACC,IAAI,EAAC;EACZ,CAAC;EACDC,OAAO,EAAE;IACPC,GAAG,GAAE,CAAC,CAAC;IACPF,IAAI,GAAG;MACLJ,OAAO,CAACO,GAAG,CAAC,OAAO,EAAE;QACnBC,MAAM,EAAE;UACNC,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,OAAO,EAAE,IAAI,CAACA;QAChB;MACF,CAAC,CAAC,CAACC,IAAI,CAACC,GAAE,IAAK;QACbC,OAAO,CAACC,GAAG,CAACF,GAAG;QACf,IAAI,CAACG,SAAQ,GAAIH,GAAG,CAACI,IAAI,CAACC